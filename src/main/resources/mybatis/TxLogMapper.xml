<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.inspien.fb.mapper.TxLogMapper">
    <sql id="TxLogColums">
        TxIdx,
        CustId,
        TxDate,
        TelegramNo,
        TxType,
        BankCd,
        Size,
        RoundTrip,
        StmtCnt,
        Status,
        StartDT,
        EndDT,
        EncData
    </sql>

    <insert id="logAdd" parameterType="TxLog">

        INSERT INTO txlog(<include refid="TxLogColums" />) values(
                  #{TxIdx},#{CustId},#{TxDate},#{TelegramNo},#{TxType},#{BankCd},
                     #{Size},#{RoundTrip},#{StmtCnt},#{Status},#{StartDT},#{EndDT},#{EncData})
    </insert>
</mapper>